<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.babyeducator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BabyEducator">
        <activity android:name=".AnimalSoundActivity"></activity>
        <activity android:name=".FruitsActivity" />
        <activity android:name=".NumbersActivity" />
        <activity android:name=".ColorsActivity" />
        <activity android:name=".ShapesActivity" />
        <activity android:name=".LetterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>